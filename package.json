{
  "name": "typescript-features",
  "version": "1.0.0",
  "description": "TypeScript Features",
  "main": "index.js",
  "scripts": {
    "start:prod:ts": "tsc --watch",
    "start:prod:server": "nodemon build/app.js",
    "start": "set NODE_ENV=prod && concurrently npm:start:prod:*",
    "start:dev:ts": "tsc --watch",
    "start:dev:server": "nodemon build/app.js",
    "start:dev": "set NODE_ENV=dev && concurrently npm:start:dev:*",
    "start:debug:ts": "tsc --watch",
    "start:debug:server": "nodemon --inspect build/app.js",
    "start:debug": "set NODE_ENV=dev && concurrently npm:start:debug:*",
    "clean": "npx rimraf ./build",
    "build": "tsc",
    "eslint": "eslint --ext .ts --fix ./src -c .eslintrc.json",
    "prettier:fix": "prettier -w ./src",
    "prettier:check": "prettier -c ./src",
    "test": "mocha",
    "test:watch": "mocha --watch",
    "test:report": "mocha --require mochawesome/register --reporter mochawesome --reporter-options reportDir=test-report,reportFilename=test-report",
    "test:coverage": "nyc --reporter=html mocha"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.2",
    "@types/chai": "^4.3.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "4.17.13",
    "@types/mocha": "^9.1.1",
    "@types/mochawesome": "^6.2.1",
    "@types/node": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "chai": "^4.3.6",
    "concurrently": "^7.2.2",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "^10.0.0",
    "mochawesome": "^7.1.3",
    "nodemon": "^2.0.19",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "4.18.1"
  }
}
